configure
set system host-name 'az-inside'

# Uplink to SDPC
set interfaces ethernet eth0 description 'UPLINK-SDPC'
set interfaces ethernet eth0 address '201.0.1.2/24'

set interfaces ethernet eth1 description 'DOMAIN'
set interfaces ethernet eth1 address '201.1.0.1/24'

set interfaces ethernet eth2 description 'DOMSVC'
set interfaces ethernet eth2 address '201.1.1.1/24'

set protocols static route '0.0.0.0/0' next-hop '201.0.1.1'
commit
save
exit
