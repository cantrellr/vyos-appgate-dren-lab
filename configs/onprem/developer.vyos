configure
set system host-name 'onp-dev'

set interfaces ethernet eth0 description 'UPLINK-SDPC'
set interfaces ethernet eth0 address '20.0.0.3/24'

set interfaces ethernet eth1 description 'DEV'
set interfaces ethernet eth1 address '20.2.0.1/24'

set interfaces ethernet eth2 description 'DEVSVC'
set interfaces ethernet eth2 address '20.2.1.1/24'

set protocols static route '0.0.0.0/0' next-hop '20.0.0.1'
commit
save
exit
