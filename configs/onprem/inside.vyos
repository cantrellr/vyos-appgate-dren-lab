configure
set system host-name 'onp-inside'

set interfaces ethernet eth0 description 'UPLINK-SDPC'
set interfaces ethernet eth0 address '202.0.1.2/24'

set interfaces ethernet eth1 description 'DOMAIN'
set interfaces ethernet eth1 address '202.1.0.1/24'

set interfaces ethernet eth2 description 'DOMSVC'
set interfaces ethernet eth2 address '202.1.1.1/24'

set interfaces ethernet eth3 description 'DEV'
set interfaces ethernet eth3 address '202.1.2.1/24'

set interfaces ethernet eth4 description 'DEVSVC'
set interfaces ethernet eth4 address '202.1.3.1/24'

set interfaces ethernet eth5 description 'SEG'
set interfaces ethernet eth5 address '202.1.4.1/24'

set interfaces ethernet eth6 description 'HWIL'
set interfaces ethernet eth6 address '202.1.5.1/24'

set protocols static route '0.0.0.0/0' next-hop '202.0.1.1'
commit
save
exit
